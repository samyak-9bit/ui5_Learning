<!-- ProductList.view.xml -->
<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="ui5.walkthrough.controller.ProductList" xmlns:core="sap.ui.core">
    <Page 
    title="People List">
       <content>
           <List items="{ productModel>/value }">
              <StandardListItem title="{productModel>FirstName}"></StandardListItem>
           </List>
       </content>
    </Page>
</mvc:View>
